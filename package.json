{
	"name": "spa-template-thin",
	"version": "1.0.0",
	"repository": "https://github.com/yanzhangshuai/spa-template-thin.git",
	"author": "yanzhangshuai<yanzhangshuai@126.com>",
	"license": "MIT",
	"scripts": {
		"vite-build": "  vite build --config ./build/vite.config.ts",
		"dev": "         vite --config ./build/vite.config.ts",
		"build": "       npm-run-all vite-build",
		"build:br": "    npm-run-all cross-env VITE_BUILD_COMPRESS=brotli npm-run-all vite-build",
		"build:gz": "    npm-run-all cross-env VITE_BUILD_COMPRESS=gzip   npm-run-all vite-build",
		"serve": "       npm-run-all vite-build && vite preview --config ./build/vite.preview.ts",
		"deploy": "      npm-run-all build:gz && docker build -t spa-template-thin -f ./deploy/Dockerfile .",
		"lint:js": "     eslint    ./{src,test,type,build}/**/*.{js,ts,tsx} --fix",
		"lint:style": "  stylelint ./src/**/*.{css,less}           --fix",
		"lint": "        npm-run-all --parallel lint:*",
		"test": "        vitest --passWithNoTests --config ./test/vitest.config.ts",
		"report": "      cross-env VITE_REPORT=true vite build --config ./build/vite.config.ts",
		"report:test": " vitest run --passWithNoTests --config ./test/vitest.config.ts --coverage  && open-cli ./report/test/index.html"
	},
	"dependencies": {
		"@mwjz/utils": "^0.1.8",
		"ahooks": "^3.7.7",
		"axios": "^1.4.0",
		"crypto-js": "^4.1.1",
		"dayjs": "^1.11.7",
		"less": "^4.1.3",
		"query-string": "^8.1.0",
		"ramda": "^0.29.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"rxjs": "^7.8.1",
		"ts-debounce": "^4.0.0",
		"typescript": "^5.0.4",
		"unocss": "^0.51.13"
	},
	"devDependencies": {
		"@antfu/eslint-config-react": "^0.38.6",
		"@rollup/plugin-commonjs": "^25.0.0",
		"@rollup/plugin-node-resolve": "^15.0.2",
		"@types/crypto-js": "^4.1.1",
		"@types/node": "^20.2.0",
		"@types/ramda": "^0.29.1",
		"@types/react": "^18.2.6",
		"@types/react-dom": "^18.2.4",
		"@typescript-eslint/eslint-plugin": "^5.59.6",
		"@typescript-eslint/parser": "^5.59.6",
		"@unocss/preset-uno": "^0.51.13",
		"@unocss/vite": "^0.51.13",
		"@vitejs/plugin-react": "^4.0.0",
		"autoprefixer": "^10.4.14",
		"c8": "^7.13.0",
		"cross-env": "^7.0.3",
		"cssnano": "^6.0.1",
		"eslint": "^8.40.0",
		"glob": "^10.2.5",
		"jsdom": "^22.0.0",
		"lint-staged": "^13.2.2",
		"npm-run-all": "^4.1.5",
		"open-cli": "^7.2.0",
		"postcss": "^8.4.23",
		"postcss-html": "^1.5.0",
		"postcss-less": "^6.0.0",
		"rollup": "^3.22.0",
		"rollup-plugin-visualizer": "^5.9.0",
		"stylelint": "^15.6.2",
		"stylelint-config-html": "^1.1.0",
		"stylelint-config-standard": "^33.0.0",
		"ts-node": "^10.9.1",
		"unocss-preset-theme": "^0.6.0",
		"vite": "^4.3.7",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-html": "^3.2.0",
		"vite-plugin-static-copy": "^0.15.0",
		"vitest": "^0.31.1"
	},
	"eslintConfig": {
		"extends": "./standard/.eslintrc.js"
	},
	"stylelint": {
		"extends": "./standard/.stylelintrc.js"
	}
}
